{
  "name": "Music Downloader",
  "version": "0.1",
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "webRequest",
    "downloads",
    "tabs",
    "http://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "name": "Music Downloader",
    "default_icon":"icon.png"
  },
    "icons": { "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png" },
  "manifest_version": 2
}
